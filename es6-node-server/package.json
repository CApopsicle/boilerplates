{
  "name": "tx-server-es6",
  "version": "0.0.1",
  "description": "TimeXchange Server",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node -- src/server.js",
    "test": "mocha -r setupTest.js --compilers js:babel-register src/**/*.test.js || exit 0",
    "test:coverage": "nyc npm run test",
    "test:report": "nyc report --reporter=text-lcov > coverage.lcov && codeclimate-test-reporter < coverage.lcov",
    "clean": "rm -rf build",
    "build": "babel src -d build",
    "build:production": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/timeexchange/timeexchangeserver.git"
  },
  "author": "lulalachen",
  "license": "MIT",
  "homepage": "https://bitbucket.org/timeexchange/timeexchangeserver#readme",
  "dependencies": {
    "es6-promise": "^3.2.1",
    "express": "^4.13.4",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "expect": "^1.20.1",
    "fetch": "^1.0.1",
    "http-client": "^4.0.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4"
  }
}
